{
  "name": "universal-react-app",
  "version": "1.1.0",
  "description": "Universal react app counter example",
  "main": "src/server/index.js",
  "config": {
    "scheme": "http",
    "domain": "localhost",
    "port": "3000"
  },
  "scripts": {
    "start": "npm run build:server",
    "build:server": "webpack --config=webpack.server.babel.js",
    "build:server:prod": "webpack --config=webpack.server.babel.js --env.production",
    "build:client": "webpack --config=webpack.client.babel.js",
    "build:client:prod": "webpack --config=webpack.client.babel.js --env.production",
    "start:prod": "node dist/server.js",
    "prettier": "prettier-eslint --write --list-different \"@(src|test)/**/*.js\" \"example/src/**/*.js\" \"@packages/(src|test)/**/*.js\"",
    "lint": "npm run prettier",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test mocha \"packages/*/test/**/*.js\" --compilers js:@babel/register",
    "prepare": "lerna bootstrap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larriereguichet/universal-react-app.git"
  },
  "author": "lutangar <johan.dufour@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.35",
    "@babel/register": "^7.0.0-beta.35",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^8.0.0-beta.0",
    "babel-preset-lag": "^0.1.2",
    "eslint": "^4.12.0",
    "eslint-config-lag": "0.0.9",
    "eslint-loader": "^1.9.0",
    "expect": "^22.0.0",
    "extract-npm-package-config": "^0.1.2",
    "lerna": "^2.5.1",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "nodemon-webpack-plugin": "^0.1.5",
    "prettier": "^1.9.2",
    "prettier-eslint-cli": "^4.4.0",
    "progress-bar-webpack-plugin": "^1.10.0",
    "react-scripts": "^1.0.17",
    "redux-logger": "^3.0.6",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^3.10.0",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "helmet": "^3.9.0",
    "history": "^4.7.2",
    "jss": "^9.3.3",
    "jss-preset-default": "^4.0.1",
    "material-ui": "^1.0.0-beta.21",
    "material-ui-icons": "^1.0.0-beta.17",
    "pino": "^4.10.2",
    "pino-http": "^3.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "reset-jss": "^1.0.0",
    "sqwish": "^0.2.2",
    "typeface-roboto": "0.0.45"
  }
}
