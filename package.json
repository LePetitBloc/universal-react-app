{
  "name": "universal-react-app",
  "version": "2.1.9",
  "description": "Universal react app counter example",
  "main": "src/server/index.js",
  "config": {
    "scheme": "http",
    "domain": "localhost",
    "port": "3000"
  },
  "scripts": {
    "start": "npm run build:server",
    "build:server": "webpack --config=webpack.server.babel.js",
    "build:server:prod": "webpack --config=webpack.server.babel.js --env.production",
    "build:client": "webpack --config=webpack.client.babel.js",
    "build:client:prod": "webpack --config=webpack.client.babel.js --env.production",
    "start:prod": "node dist/server.js",
    "fix": "prettier-eslint --write --list-different \"@(src|test)/**/*.js\" \"./webpack.server.babel.js\" \"packages/*/@(src|test|rules)/**/*.js\" \"packages/*/index.js\"",
    "lint": "eslint \"@(src|test)/**/*.js\" \"./webpack.server.babel.js\" \"packages/*/@(src|test|rules)/**/*.js\" \"packages/*/index.js\"",
    "pretest": "npm run fix",
    "test": "NODE_ENV=test mocha \"packages/*/test/**/*.js\" --compilers js:@babel/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larriereguichet/universal-react-app.git"
  },
  "author": "lutangar <johan.dufour@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.38",
    "@babel/register": "^7.0.0-beta.38",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^8.0.0-beta.0",
    "babel-preset-lag": "^2.1.9",
    "eslint": "^4.12.0",
    "eslint-config-lag": "0.0.9",
    "eslint-loader": "^1.9.0",
    "lerna": "^2.5.1",
    "mocha": "^4.1.0",
    "prettier": "^1.9.2",
    "prettier-eslint-cli": "^4.4.0",
    "react-dev-middleware": "^2.1.9",
    "redux-logger": "^3.0.6",
    "webpack": "^3.10.0",
    "webpack-config-lag": "^2.1.9"
  },
  "dependencies": {
    "dom-remove": "^1.1.0",
    "history": "^4.7.2",
    "jss": "^9.3.3",
    "jss-preset-default": "^4.0.1",
    "material-ui": "^1.0.0-beta.27",
    "material-ui-icons": "^1.0.0-beta.17",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.1.1",
    "react-express-server": "^2.1.9",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "next",
    "react-jss": "^8.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "reset-jss": "^1.0.0",
    "server-hot-loader": "^2.1.9",
    "typeface-roboto": "0.0.45"
  }
}
